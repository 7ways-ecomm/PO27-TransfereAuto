        },
        {
          "image": "assets/banners/mock/105x105.png",
          "mobileImage": "assets/banners/mock/105x105.png",
          "description": "something something"
        },
        {
          "image": "assets/banners/mock/105x105.png",
          "mobileImage": "assets/banners/mock/105x105.png",
          "description": "something something"
        },
        {
          "image": "assets/banners/mock/105x105.png",
          "mobileImage": "assets/banners/mock/105x105.png",
          "description": "something something"
        },
        {
          "image": "assets/banners/mock/105x105.png",
          "mobileImage": "assets/banners/mock/105x105.png",
          "description": "something something"
        },
        {
          "image": "assets/banners/mock/105x105.png",
          "mobileImage": "assets/banners/mock/105x105.png",
          "description": "something something"
        }
      ]
    }
  },
  "slider-layout#home-slider-types": {
    "props": {
      "itemsPerPage": {
        "desktop": 6,
        "tablet": 3,
        "phone": 2
      },
      "centerMode": {
        "phone": "to-the-left",
        "desktop": "disabled"
      },
      "centerModeSlidesGap": 20,
      "infinite": true,
      "blockClass": "slider-types",
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "fullWidth": false,
      "arrowSize": 30
    }
  }
