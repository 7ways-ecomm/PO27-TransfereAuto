{
  "flex-layout.row#home-slider-types": {
    "title": "Tipos de Produto",
    "children": [
      "rich-text#home-types-title",
      "list-context.image-list#home-slider-types"
    ],
    "props": {
      "blockClass": [
        "home-slider-types"
      ],
      "colSizing": "auto",
      "horizontalAlign": "center",
      "preventVerticalStretch": true
    }
  },
  "rich-text#home-types-title": {
    "title": "Título Tipos",
    "props": {
      "text": "Produtos especiais para o seu estilo de alimentação",
      "blockClass": [
        "home-types-title",
        "title-default"
      ]
    }
  },
  "list-context.image-list#home-slider-types": {
    "title": "Slider",
    "children": [
      "slider-layout#home-slider-types"
    ],
    "props": {
      "images": [
        {
