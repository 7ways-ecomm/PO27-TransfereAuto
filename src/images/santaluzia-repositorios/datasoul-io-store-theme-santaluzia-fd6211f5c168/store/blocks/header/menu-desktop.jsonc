    "title": "Primeiro nível - Items",
    "props": {
      "blockClass": [
        "item-desktop"
      ]
    }
  },
  // Menu departamentos
  "flex-layout.row#menu-main": {
    "children": [
      "menu-item#menu-title"
    ],
    "props": {
      "blockClass": [
        "menu-main"
      ],
      "colSizing": "auto"
    }
  },
  "menu-item#menu-title": {
    "props": {
      "blockClass": [
        "menu-main-title"
