{
  "flex-layout.row#menu": {
    "title": "Menu - Desktop",
    "children": [
      "flex-layout.row#menu-main",
      "vtex.menu@2.x:menu#menu-items"
    ],
    "props": {
      "blockClass": [
        "menu-container__desktop"
